{#
/**
 * @file
 * Theme override to display a region.
 *
 * Available variables:
 * - content: The content for this region, typically blocks.
 * - attributes: HTML attributes for the region div.
 * - region: The name of the region variable as defined in the theme's
 *   .info.yml file.
 *
 * @see template_preprocess_region()
 */
#}
{% if content %}
  <div class="footer bg-gray-900">
    <div class="container">
      <div class="row pt-5 pb-5 justify-content-center">
        <div class="col-12 mt-3 mb-3">
          <p class="text-white text-center">{{ content }}</p>
        </div>
        <div class="col-6 col-sm-auto"><a href="https://aarhus.dk"><img src="/{{ directory }}/images/aak_kb_logo_negative.svg" alt="Aarhus Kommune Kutur og Borgerservice"></a></div>
        <div class="col-6 col-sm-auto"><a href="https://stadsarkiv.aarhus.dk/"><img src="/{{ directory }}/images/as_logo_white_51x187.png" alt="Aarhus Stadsarkiv"></a></div>
        <div class="col-12 mt-3 mb-3">
          <ul class="nav justify-content-center">
            <li class="nav-item">
              <a class="nav-link" href="https://aarhus.dk">aarhus.dk</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="https://stadsarkiv.aarhus.dk/">stadsarkiv.aarhus.dk</a>
            </li>
          </ul>
        </div>
      </div>

    </div>
  </div>
{% endif %}
